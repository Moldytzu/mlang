%include 'stdlib/standard.mlsrc'

 # push ABCD on memory #
0 while dp 4 < do # while 0 is smaller than 4 do #
    dp 65 + pushMemory m+ # push on memory character 65 + variable ('A'+alphabet character) #
    dp 1 + # increase variable #
end

10 pushMemory m+ # push \n #

# go back 4 values #
0 while dp 5 < do 
    dp 1 +
    m-
end

0 while dp 5 < do
    dp 1 +
    popMemory display m+
end
